### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#e2e1df'
set completion-bg '#0C0707'

# Command line completion group elements
set completion-group-fg '#AD847D'
set completion-group-bg '#0C0707'

# Current command line completion element
set completion-highlight-fg '#0C0707'
set completion-highlight-bg '#e2e1df'

# Default foreground/background color
set default-bg rgba(12,7,7,1)

# Input bar
set inputbar-fg '#e2e1df'
set inputbar-bg '#0C0707'

# Notification
set notification-fg '#e2e1df'
set notification-bg '#0C0707'

# Error notification
set notification-error-fg '#e2e1df'
set notification-error-bg '#BD6B63'

# Warning notification
set notification-warning-fg '#e2e1df'
set notification-warning-bg '#BD6B63'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#e2e1df'
set statusbar-bg '#0C0707'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#AD847D'
set highlight-active-color '#AD847D'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#e2e1df'

# 'Loading...' text
set render-loading-fg '#e2e1df'
set render-loading-bg '#0C0707'

# Index mode
set index-fg '#e2e1df'
set index-bg '#0C0707'

# Selected element in index mode
set index-active-fg '#0C0707'
set index-active-bg '#e2e1df'