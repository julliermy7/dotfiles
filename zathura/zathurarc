### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c19c9f'
set completion-bg '#0d0807'

# Command line completion group elements
set completion-group-fg '#91282E'
set completion-group-bg '#0d0807'

# Current command line completion element
set completion-highlight-fg '#0d0807'
set completion-highlight-bg '#c19c9f'

# Default foreground/background color
set default-bg rgba(13,8,7,1)

# Input bar
set inputbar-fg '#c19c9f'
set inputbar-bg '#0d0807'

# Notification
set notification-fg '#c19c9f'
set notification-bg '#0d0807'

# Error notification
set notification-error-fg '#c19c9f'
set notification-error-bg '#9C1519'

# Warning notification
set notification-warning-fg '#c19c9f'
set notification-warning-bg '#9C1519'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c19c9f'
set statusbar-bg '#0d0807'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#91282E'
set highlight-active-color '#91282E'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c19c9f'

# 'Loading...' text
set render-loading-fg '#c19c9f'
set render-loading-bg '#0d0807'

# Index mode
set index-fg '#c19c9f'
set index-bg '#0d0807'

# Selected element in index mode
set index-active-fg '#0d0807'
set index-active-bg '#c19c9f'