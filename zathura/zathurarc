### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#cadce5'
set completion-bg '#050611'

# Command line completion group elements
set completion-group-fg '#358CBA'
set completion-group-bg '#050611'

# Current command line completion element
set completion-highlight-fg '#050611'
set completion-highlight-bg '#cadce5'

# Default foreground/background color
set default-bg rgba(5,6,17,1)

# Input bar
set inputbar-fg '#cadce5'
set inputbar-bg '#050611'

# Notification
set notification-fg '#cadce5'
set notification-bg '#050611'

# Error notification
set notification-error-fg '#cadce5'
set notification-error-bg '#1766AC'

# Warning notification
set notification-warning-fg '#cadce5'
set notification-warning-bg '#1766AC'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#cadce5'
set statusbar-bg '#050611'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#358CBA'
set highlight-active-color '#358CBA'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#cadce5'

# 'Loading...' text
set render-loading-fg '#cadce5'
set render-loading-bg '#050611'

# Index mode
set index-fg '#cadce5'
set index-bg '#050611'

# Selected element in index mode
set index-active-fg '#050611'
set index-active-bg '#cadce5'