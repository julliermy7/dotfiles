### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#e3d2bc'
set completion-bg '#0d1012'

# Command line completion group elements
set completion-group-fg '#AC8F6A'
set completion-group-bg '#0d1012'

# Current command line completion element
set completion-highlight-fg '#0d1012'
set completion-highlight-bg '#e3d2bc'

# Default foreground/background color
set default-bg rgba(13,16,18,1)

# Input bar
set inputbar-fg '#e3d2bc'
set inputbar-bg '#0d1012'

# Notification
set notification-fg '#e3d2bc'
set notification-bg '#0d1012'

# Error notification
set notification-error-fg '#e3d2bc'
set notification-error-bg '#538876'

# Warning notification
set notification-warning-fg '#e3d2bc'
set notification-warning-bg '#538876'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#e3d2bc'
set statusbar-bg '#0d1012'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#AC8F6A'
set highlight-active-color '#AC8F6A'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#e3d2bc'

# 'Loading...' text
set render-loading-fg '#e3d2bc'
set render-loading-bg '#0d1012'

# Index mode
set index-fg '#e3d2bc'
set index-bg '#0d1012'

# Selected element in index mode
set index-active-fg '#0d1012'
set index-active-bg '#e3d2bc'