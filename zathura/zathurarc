### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#eee9d5'
set completion-bg '#0e1012'

# Command line completion group elements
set completion-group-fg '#B6A98F'
set completion-group-bg '#0e1012'

# Current command line completion element
set completion-highlight-fg '#0e1012'
set completion-highlight-bg '#eee9d5'

# Default foreground/background color
set default-bg rgba(14,16,18,1)

# Input bar
set inputbar-fg '#eee9d5'
set inputbar-bg '#0e1012'

# Notification
set notification-fg '#eee9d5'
set notification-bg '#0e1012'

# Error notification
set notification-error-fg '#eee9d5'
set notification-error-bg '#A1927E'

# Warning notification
set notification-warning-fg '#eee9d5'
set notification-warning-bg '#A1927E'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#eee9d5'
set statusbar-bg '#0e1012'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#B6A98F'
set highlight-active-color '#B6A98F'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#eee9d5'

# 'Loading...' text
set render-loading-fg '#eee9d5'
set render-loading-bg '#0e1012'

# Index mode
set index-fg '#eee9d5'
set index-bg '#0e1012'

# Selected element in index mode
set index-active-fg '#0e1012'
set index-active-bg '#eee9d5'