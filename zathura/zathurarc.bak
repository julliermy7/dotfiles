### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#f5f5f4'
set completion-bg '#3d271c'

# Command line completion group elements
set completion-group-fg '#B2A996'
set completion-group-bg '#3d271c'

# Current command line completion element
set completion-highlight-fg '#3d271c'
set completion-highlight-bg '#f5f5f4'

# Default foreground/background color
set default-bg rgba(61,39,28,1)

# Input bar
set inputbar-fg '#f5f5f4'
set inputbar-bg '#3d271c'

# Notification
set notification-fg '#f5f5f4'
set notification-bg '#3d271c'

# Error notification
set notification-error-fg '#f5f5f4'
set notification-error-bg '#B67C83'

# Warning notification
set notification-warning-fg '#f5f5f4'
set notification-warning-bg '#B67C83'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#f5f5f4'
set statusbar-bg '#3d271c'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#B2A996'
set highlight-active-color '#B2A996'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#f5f5f4'

# 'Loading...' text
set render-loading-fg '#f5f5f4'
set render-loading-bg '#3d271c'

# Index mode
set index-fg '#f5f5f4'
set index-bg '#3d271c'

# Selected element in index mode
set index-active-fg '#3d271c'
set index-active-bg '#f5f5f4'