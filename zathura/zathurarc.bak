### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#e2a9bb'
set completion-bg '#0d0c14'

# Command line completion group elements
set completion-group-fg '#D2106B'
set completion-group-bg '#0d0c14'

# Current command line completion element
set completion-highlight-fg '#0d0c14'
set completion-highlight-bg '#e2a9bb'

# Default foreground/background color
set default-bg rgba(13,12,20,1)

# Input bar
set inputbar-fg '#e2a9bb'
set inputbar-bg '#0d0c14'

# Notification
set notification-fg '#e2a9bb'
set notification-bg '#0d0c14'

# Error notification
set notification-error-fg '#e2a9bb'
set notification-error-bg '#9B2260'

# Warning notification
set notification-warning-fg '#e2a9bb'
set notification-warning-bg '#9B2260'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#e2a9bb'
set statusbar-bg '#0d0c14'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#D2106B'
set highlight-active-color '#D2106B'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#e2a9bb'

# 'Loading...' text
set render-loading-fg '#e2a9bb'
set render-loading-bg '#0d0c14'

# Index mode
set index-fg '#e2a9bb'
set index-bg '#0d0c14'

# Selected element in index mode
set index-active-fg '#0d0c14'
set index-active-bg '#e2a9bb'