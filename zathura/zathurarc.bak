### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#ded7ca'
set completion-bg '#0d0604'

# Command line completion group elements
set completion-group-fg '#9F7154'
set completion-group-bg '#0d0604'

# Current command line completion element
set completion-highlight-fg '#0d0604'
set completion-highlight-bg '#ded7ca'

# Default foreground/background color
set default-bg rgba(13,6,4,1)

# Input bar
set inputbar-fg '#ded7ca'
set inputbar-bg '#0d0604'

# Notification
set notification-fg '#ded7ca'
set notification-bg '#0d0604'

# Error notification
set notification-error-fg '#ded7ca'
set notification-error-bg '#935C2D'

# Warning notification
set notification-warning-fg '#ded7ca'
set notification-warning-bg '#935C2D'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#ded7ca'
set statusbar-bg '#0d0604'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#9F7154'
set highlight-active-color '#9F7154'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#ded7ca'

# 'Loading...' text
set render-loading-fg '#ded7ca'
set render-loading-bg '#0d0604'

# Index mode
set index-fg '#ded7ca'
set index-bg '#0d0604'

# Selected element in index mode
set index-active-fg '#0d0604'
set index-active-bg '#ded7ca'