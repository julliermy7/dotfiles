### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#edcba1'
set completion-bg '#131818'

# Command line completion group elements
set completion-group-fg '#9A9119'
set completion-group-bg '#131818'

# Current command line completion element
set completion-highlight-fg '#131818'
set completion-highlight-bg '#edcba1'

# Default foreground/background color
set default-bg rgba(19,24,24,1)

# Input bar
set inputbar-fg '#edcba1'
set inputbar-bg '#131818'

# Notification
set notification-fg '#edcba1'
set notification-bg '#131818'

# Error notification
set notification-error-fg '#edcba1'
set notification-error-bg '#AA5828'

# Warning notification
set notification-warning-fg '#edcba1'
set notification-warning-bg '#AA5828'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#edcba1'
set statusbar-bg '#131818'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#9A9119'
set highlight-active-color '#9A9119'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#edcba1'

# 'Loading...' text
set render-loading-fg '#edcba1'
set render-loading-bg '#131818'

# Index mode
set index-fg '#edcba1'
set index-bg '#131818'

# Selected element in index mode
set index-active-fg '#131818'
set index-active-bg '#edcba1'