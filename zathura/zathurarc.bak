### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#cdcdcd'
set completion-bg '#0C0C0C'

# Command line completion group elements
set completion-group-fg '#95231E'
set completion-group-bg '#0C0C0C'

# Current command line completion element
set completion-highlight-fg '#0C0C0C'
set completion-highlight-bg '#cdcdcd'

# Default foreground/background color
set default-bg rgba(12,12,12,1)

# Input bar
set inputbar-fg '#cdcdcd'
set inputbar-bg '#0C0C0C'

# Notification
set notification-fg '#cdcdcd'
set notification-bg '#0C0C0C'

# Error notification
set notification-error-fg '#cdcdcd'
set notification-error-bg '#6E6E6E'

# Warning notification
set notification-warning-fg '#cdcdcd'
set notification-warning-bg '#6E6E6E'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#cdcdcd'
set statusbar-bg '#0C0C0C'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#95231E'
set highlight-active-color '#95231E'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#cdcdcd'

# 'Loading...' text
set render-loading-fg '#cdcdcd'
set render-loading-bg '#0C0C0C'

# Index mode
set index-fg '#cdcdcd'
set index-bg '#0C0C0C'

# Selected element in index mode
set index-active-fg '#0C0C0C'
set index-active-bg '#cdcdcd'